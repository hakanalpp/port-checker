{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier/prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-use-before-define": "warn",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}
